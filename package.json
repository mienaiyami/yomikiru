{
    "name": "yomikiru",
    "productName": "Yomikiru",
    "version": "2.22.3",
    "description": "An offline desktop reader for manga, comics, and novels. Offers a customizable reading experience with extensive settings, shortcuts, themes and layouts.",
    "main": ".webpack/main",
    "author": {
        "name": "mienaiyami",
        "url": "https://github.com/mienaiyami/yomikiru"
    },
    "scripts": {
        "dev": "electron-forge start",
        "rebuild:electron": "electron-rebuild -f -w better-sqlite3",
        "rebuild:node": "pnpm rebuild better-sqlite3",
        "drizzle": "cross-env ELECTRON_RUN_AS_NODE=1 electron ./node_modules/drizzle-kit/bin.cjs",
        "drizzle:push": "pnpm drizzle push",
        "drizzle:studio": "pnpm drizzle studio",
        "drizzle:generate": "pnpm drizzle generate",
        "drizzle:migrate": "pnpm drizzle migrate",
        "biome:check": "biome check .",
        "biome:check-error": "biome check . --diagnostic-level error",
        "biome:fix": "biome check --write .",
        "format": "biome format --write .",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "check": "biome check . && tsc --noemit",
        "check:chrome108": "eslint src --ext .js,.jsx,.ts,.tsx --config .eslintrc.chrome108.json",
        "check:full": "biome check . --diagnostic-level error && eslint --config .eslintrc.chrome108.json && tsc --noemit",
        "tslint": "tsc --noemit",
        "package": "electron-forge package",
        "make:exe32": "electron-forge make --targets=@electron-forge/maker-squirrel --arch=ia32",
        "make:exe64": "electron-forge make --targets=@electron-forge/maker-squirrel --arch=x64",
        "make:zip32": "electron-forge make --targets=@electron-forge/maker-zip --arch=ia32",
        "make:zip64": "electron-forge make --targets=@electron-forge/maker-zip --arch=x64",
        "make:win32": "electron-forge make --targets=@electron-forge/maker-squirrel,@electron-forge/maker-zip --arch=ia32",
        "make:win64": "electron-forge make --targets=@electron-forge/maker-squirrel,@electron-forge/maker-zip --arch=x64",
        "make:deb": "electron-forge make --targets=@electron-forge/maker-deb",
        "release": "node tagAndPush.js",
        "prepare": "husky"
    },
    "license": "MIT",
    "dependencies": {
        "@electron/remote": "^2.0.1",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-brands-svg-icons": "^6.2.1",
        "@fortawesome/free-regular-svg-icons": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@reduxjs/toolkit": "^1.9.3",
        "@vscode/sudo-prompt": "^9.3.1",
        "better-sqlite3": "^11.8.1",
        "chokidar": "^3.5.3",
        "color": "^4.2.3",
        "cross-zip": "^4.0.0",
        "css": "^3.0.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.39.3",
        "drizzle-zod": "^0.7.0",
        "electron-dl": "^3.5.0",
        "electron-fetch": "^1.9.1",
        "electron-log": "^4.4.8",
        "electron-squirrel-startup": "^1.0.0",
        "font-list": "^1.4.5",
        "pdfjs-dist": "https://github.com/mienaiyami/pdfjs-dist",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-focus-lock": "^2.9.5",
        "react-hot-loader": "^4.13.0",
        "react-intersection-observer": "^9.5.2",
        "react-redux": "^8.0.5",
        "semver": "^7.7.1",
        "zod": "^3.22.3"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.5",
        "@electron-forge/cli": "^6.4.1",
        "@electron-forge/maker-deb": "^6.4.1",
        "@electron-forge/maker-rpm": "^6.4.1",
        "@electron-forge/maker-squirrel": "^6.4.1",
        "@electron-forge/maker-zip": "^6.4.1",
        "@electron-forge/plugin-auto-unpack-natives": "^6.4.1",
        "@electron-forge/plugin-webpack": "^6.4.1",
        "@electron-forge/shared-types": "^7.6.1",
        "@electron/rebuild": "^3.7.1",
        "@types/better-sqlite3": "^7.6.12",
        "@types/color": "^3.0.3",
        "@types/cross-zip": "^4.0.0",
        "@types/css": "^0.0.33",
        "@types/electron-devtools-installer": "2.2.2",
        "@types/node": "^18.11.18",
        "@types/pdfjs-dist": "^2.10.378",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@types/semver": "^7.7.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.3",
        "cross-env": "7.0.3",
        "css-loader": "^6.4.0",
        "drizzle-kit": "^0.30.4",
        "electron": "22.3.25",
        "eslint": "7.29.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "^3.8.2",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "7.24.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.3",
        "node-loader": "^2.0.0",
        "npm-run-all": "4.1.5",
        "prettier": "^3.5.3",
        "sass": "^1.43.2",
        "sass-loader": "^12.2.0",
        "style-loader": "^3.3.0",
        "ts-loader": "^9.2.2",
        "ts-node": "^10.0.0",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "tsx": "^4.19.2",
        "typescript": "^5.1.6",
        "url-loader": "^4.1.1",
        "wait-on": "5.3.0",
        "webpack": "^5.98.0",
        "worker-loader": "^3.0.8"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "biome check --write --files-ignore-unknown=true --no-errors-on-unmatched"
        ],
        "*.{json,css,md}": [
            "biome format --write"
        ]
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@fortawesome/fontawesome-common-types",
            "@fortawesome/fontawesome-svg-core",
            "@fortawesome/free-brands-svg-icons",
            "@fortawesome/free-regular-svg-icons",
            "@fortawesome/free-solid-svg-icons",
            "better-sqlite3",
            "canvas",
            "electron",
            "electron-winstaller",
            "esbuild"
        ]
    }
}
